end

local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild('HumanoidRootPart')

print('üîç B·∫Øt ƒë·∫ßu teleport + collect RewardPoint1 ‚Üí RewardPoint20...')

-- üü¢ H√†m th·ª≠ claim RewardPoint
local function tryCollect(point)
    if not point or not point.Parent then
        return false
    end

    for _, descendant in ipairs(point:GetDescendants()) do
        if descendant:IsA('ProximityPrompt') then
            if descendant.Enabled == true then
                print('‚ú® Collecting:', point.Name)
                fireproximityprompt(descendant) -- b·∫•m E
                return true
            else
                print('‚ö™ Prompt ·ªü', point.Name, 'ƒë√£ b·ªã disable')
            end
        end
    end

    return false
end

while task.wait(1) do
    for i = 1, 20 do
        local point = workspace:FindFirstChild('RewardPoint' .. i)
        if point then
            -- L·∫•y v·ªã tr√≠ pivot
            local objCF
            if point:IsA('Model') and point.PrimaryPart then
                objCF = point:GetPivot()
            elseif point:IsA('BasePart') then
                objCF = point.CFrame
            end

            if objCF then
                local objPos = objCF.Position
                print('‚û°Ô∏è Teleport t·ªõi:', point.Name)

                -- Tele ra tr∆∞·ªõc 3 studs, cao h∆°n 1 stud
                local forward = objCF.LookVector * 3
                local targetPos = objPos + forward + Vector3.new(0, 1, 0)
                hrp.CFrame = CFrame.new(targetPos, objPos)

                -- Camera follow
                workspace.CurrentCamera.CFrame =
                    CFrame.new(objPos + Vector3.new(0, 10, 15), objPos)

                -- ‚è≥ Ch·ªù t√≠ r·ªìi th·ª≠ claim
                task.wait(0.5)
                local collected = tryCollect(point)
                if collected then
                    print('‚úÖ ƒê√£ l·∫•y RewardPoint:', point.Name)
                else
                    print('‚ùå Kh√¥ng c√≥ g√¨ ƒë·ªÉ l·∫•y ·ªü:', point.Name)
                end
            end
        else
            print('‚ö†Ô∏è Kh√¥ng t√¨m th·∫•y RewardPoint' .. i)
        end
    end
end
-- üåü Auto Equip Tools (Enchanted Chest + Fairy Power Extender)
local Players = game:GetService('Players')
local localPlayer = Players.LocalPlayer

-- üß∞ H√†m trang b·ªã Enchanted Chest
local function equipEnchantedChest()
    -- N·∫øu ƒë√£ c·∫ßm tr√™n tay r·ªìi th√¨ th√¥i
    for _, tool in ipairs(localPlayer.Character:GetChildren()) do
        if tool:IsA('Tool') and string.find(tool.Name, 'Fairy Summoner') then
            return tool
        end
    end

    -- N·∫øu c√≤n trong Backpack th√¨ auto equip
    for _, tool in ipairs(localPlayer.Backpack:GetChildren()) do
        if tool:IsA('Tool') and string.find(tool.Name, 'Fairy Summoner') then
            tool.Parent = localPlayer.Character
            print('üì¶ ƒê√£ auto c·∫ßm Enchanted Chest!')
            return tool
        end
    end
end

-- üß∞ H√†m trang b·ªã Fairy Power Extender
local function equipFairyPowerExtender()
    -- N·∫øu ƒë√£ c·∫ßm tr√™n tay r·ªìi th√¨ th√¥i
    for _, tool in ipairs(localPlayer.Character:GetChildren()) do
        if
            tool:IsA('Tool') and string.find(tool.Name, 'Fairy Power Extender')
        then
            return tool
        end
    end

    -- N·∫øu c√≤n trong Backpack th√¨ auto equip
    for _, tool in ipairs(localPlayer.Backpack:GetChildren()) do
        if
            tool:IsA('Tool') and string.find(tool.Name, 'Fairy Power Extender')
        then
            tool.Parent = localPlayer.Character
            print('üì¶ ƒê√£ auto c·∫ßm Fairy Power Extender!')
            return tool
        end
    end
end

-- üßö H√†m g·ªçi Fairy Summoner skill
local function fairySummoner()
    local args = {
        [1] = true,
        [2] = CFrame.new(-9.378669738769531, 0, -49.54979705810547)
            * CFrame.Angles(
                -1.340797781944275,
                1.1134178638458252,
                1.3155239820480347
            ),
    }

    local char = localPlayer.Character
    if
        char
        and char:FindFirstChild('InputGateway')
        and char.InputGateway:FindFirstChild('Activation')
    then
        char.InputGateway.Activation:FireServer(unpack(args))
        print('‚ú® Fairy Summoner skill ƒë√£ ƒë∆∞·ª£c k√≠ch ho·∫°t!')
    else
        warn('‚ö†Ô∏è Kh√¥ng t√¨m th·∫•y Activation trong InputGateway')
    end
end

-- üîÑ Lu√¥n gi·ªØ tr√™n tay & d√πng skill (m·ªói 10s)
task.spawn(function()
    while task.wait(10) do
        equipEnchantedChest()
        fairySummoner()
    end
end)
wait(3)
-- üîÑ Lu√¥n gi·ªØ tr√™n tay & d√πng skill (m·ªói 10s)
task.spawn(function()
    while task.wait(10) do
        equipFairyPowerExtender()
        fairySummoner()
    end
end)
